            <word name="prty"/> 
            <word name="replyto"/>
            <word name="resfmt"/>
            <word name="retains"/>           
            <word name="retainf"/>           
            <word name="retryl"/>           
            <word name="retryt"/>
            <word name="room"/>
            <word name="sysarea"/>
            <word name="threshld"/>                                            
            <word name="title"/>
            <word name="trc"/> 
            <word name="ucs"/>
            <word name="userdata"/>                                           
            <word name="userlib"/>                                           
            <word name="userpath"/>
            <word name="writer"/>                                                                                      
<!-- output parameters end -->
<!-- dd parameters begin-->
	    <word name="accode"/>
	    <word name="amp"/>
            <word name="avgrec"/>
	    <word name="burst"/>
	    <word name="ccsid"/>
            <word name="chars"/>
            <word name="chkpt"/>
            <word name="cntl"/>
            <word name="dataclas"/>
            <word name="dcb"/>
            <word name="ddname"/>
