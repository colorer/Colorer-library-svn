<!--
    JCL syntax. Based on:
    
    REXX Syntax 4ever
    3dcraze version was made by Vladimir Komarov <doublew@iname.com>

    Upgrade for Colorer4u and 4ever Igor Paikin <exemok@mail.ru>
                with help of Vladimir Komarov <doublew@iname.com>:
    1) adding some more keywords
    2) do .. end works as brackets
    3) also added (modified?) odinary brackets - [..] and (..)
    4) Nested comments support (is it really works?)
    5) Unneeded end's are in error color
    6) Inserted functions - thanks to Vladimir Komarov

    Thanks to:
    IBM - for that stupid language JCL
    Eugene Roshal and Far group - for FAR
    cail lomecb - for colorer and some help in writing of this file
    Vladimir Komarov <doublew@iname.com> - author of 3dcraze version of REXX syntax
                                           and soauthor of 4u and 4ever version
    You - that are use (or read, modify and send me notes) this hrc file
-->
   <type name="jcl">

      <import type="def"/>

      <region name="jclString" parent="String"/>
      <region name="jclComment" parent="Comment"/>
      <region name="jclSymb" parent="Symbol"/>
      <region name="jclWord" parent="Keyword"/>
      <region name="jclParam" parent="Parameter"/>
      <region name="jclParam2" parent="ParameterStrong"/>



      <scheme name="jcl">
<!-- Strings -->
        <regexp match="/(\&#39;(.*?[^\&#39;])??\&#39;)/" region0="jclString"/>
        <regexp match="/(\&#34;(.*?[^\&#34;])??\&#34;)/" region0="jclString"/>
<!-- Numbers  -->
         <inherit scheme="DecNumber"/>
         <keywords ignorecase="yes" region="Error">
            <word name="end"/>
<!--no begin for this end -->
            <word name="*/"/>
<!-- no open comment for this close comment -->
         </keywords>
<!-- Symbols -->
         <keywords region="jclSymb">
            <symb name="("/>
            <symb name=")"/>
            <symb name="["/>
            <symb name="]"/>
            <symb name="+"/>
            <symb name="-"/>
            <symb name="*"/>
            <symb name="/"/>
            <symb name=","/>
            <symb name="."/>
            <symb name=";"/>
            <symb name="="/>
            <symb name="!"/>
            <symb name="|"/>
 	    <symb name="GT"/>
 	    <symb name="GE"/>
 	    <symb name="EQ"/>
 	    <symb name="LT"/>
 	    <symb name="LE"/>
 	    <symb name="NE"/>
         </keywords>
<!-- Words -->
         <keywords ignorecase="yes" region="jclWord">
	    <word name="command"/>
            <word name="exec"/>
            <word name="job"/>
            <word name="dd"/>
	    <word name="jcllib"/>
	    <word name="cntl"/>
	    <word name="endcntl"/>
	    <word name="dsname"/>
	    <word name="if"/>
	    <word name="else"/>
	    <word name="then"/>
	    <word name="endif"/>
	    <word name="and"/>
	    <word name="or"/>
	    <word name="proc"/>
	    <word name="pend"/>
	    <word name="include"/>
	    <word name="set"/>
	    <word name="xmit"/>
            <word name="/*jobparam"/>
            <word name="/*message"/>
            <word name="/*metacct"/>
            <word name="/*notify"/>
            <word name="/*output"/>
            <word name="/*priority"/>
            <word name="/*route"/>
            <word name="/*setup"/>
            <word name="/*signof"/>
            <word name="/*signon"/>
            <word name="/*xeq"/>
            <word name="/*xmit"/>
         </keywords>
         <keywords ignorecase="yes" region="jclParam">
<!-- JOB operator parameters begin -->
            <word name="bytes"/>
	    <word name="cards"/>
	    <word name="ccsid"/>
            <word name="class"/>
	    <word name="group"/>
            <word name="lines"/>
            <word name="msgclass"/>
            <word name="msglevel"/>
            <word name="notify"/>
            <word name="pages"/>
            <word name="prty"/>
	    <word name="restart"/>
	    <word name="seclabel"/>
	    <word name="schev"/>
            <word name="typerun"/>
	    <word name="user"/>
<!-- JOB operator parameters begin -->
<!-- EXEC operator parameters begin -->
            <word name="acct"/>
	    <word name="addrspc"/>
	    <word name="ccsid"/>
	    <word name="cond"/>
	    <word name="dynamnbr"/>
	    <word name="parm"/>
 	    <word name="perform"/>
            <word name="pgm"/>
            <word name="proc"/>
	    <word name="rd"/>
            <word name="region"/>
	    <word name="time"/>
<!-- EXEC operator parameters end -->
            <word name="notify"/>
            <word name="order"/>
	    <word name="parm"/>
<!-- INCLUDE operator parameters -->
	    <word name="member"/>
<!-- output parameters begin -->
	    <word name="address"/>
	    <word name="affstats"/>           
	    <word name="building"/>
