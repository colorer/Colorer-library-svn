
TARGET=colorer.exe
all: $(TARGET)

shared_path=../shared/
obj_output=objs

#for profiler usage
#CPPFLAGS_CUSTOM = /Zi
CPPFLAGS_CUSTOM = -DCOLORER_FEATURE_HRC_ROMIZING=TRUE -DCOLORER_FEATURE_USE_CHUNK_ALLOC=TRUE
#-DCOLORER_FEATURE_LOGLEVEL=COLORER_FEATURE_LOGLEVEL_FULL
#LIB_DEBUG = /debug /incremental:no

include ../shared/makefile.colorer.vc

LIBFLAGS = $(objs) $(LIB_DEBUG) /map /fixed:no /out:$(TARGET) /nologo /subsystem:console wininet.lib

include makefile.core

clean:
	$(RM) $(objs) *.exp *.lib *.map *.pdb *.pdb
