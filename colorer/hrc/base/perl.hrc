    Timur Safin <timur@lanit.ru>
    cail: now using different schemes for
          different paired limiters, fulll s()() support
          needs complex variables parsing - using refs, hashs and lists
          also to correct constructions as    s@  foo  @  @bar  @ix;
-->
<!DOCTYPE hrc PUBLIC "-//Cail Lomecb//DTD Colorer HRC take5//EN" "../hrc.dtd">
<hrc version="take5"
     xmlns="http://colorer.sf.net/2003/hrc"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:schemaLocation="http://colorer.sf.net/2003/hrc ../hrc.xsd">

   <type name="perl" access="public">

      <import type="def"/>

      <region name="plString" parent="String"/>
      <region name="plStringEdge" parent="StringEdge"/>
      <region name="plComment" parent="Comment"/>
      <region name="plSymb" parent="Symbol"/>
      <region name="plMainSymb" parent="SymbolStrong"/>
      <region name="plWord" parent="Keyword"/>
      <region name="plNumb" parent="Number"/>
      <region name="plStream" parent="Path"/>
      <region name="plRare" parent="CommentContent"/>
      <region name="plSlash" parent="StringContent"/>
      <region name="plVar" parent="Var"/>
      <region name="plVar2" parent="VarStrong"/>
      <region name="plPod" parent="CommentDoc"/>
      <region name="plPodSym" parent="Symbol"/>
      <region name="plPodText" parent="default"/>
      <region name="plPodSel" parent="String"/>

      <region name="SubOutline" parent="def:Outlined"/>
      <region name="PackageOutline" parent="def:Outlined"/>

      <scheme name="perlBuildinVars">
         <keywords region="plVar">
            <word name="$&amp;"/>
            <word name="$&#34;"/>
            <word name="$'"/>
            <word name="$`"/>
            <word name="$]"/>
            <word name="$["/>
            <word name="$+"/>
            <word name="$*"/>
            <word name="$,"/>
            <word name="$/"/>
            <word name="$|"/>
            <word name="$."/>
            <word name="$\"/>
            <word name="$;"/>
            <word name="$%"/>
            <word name="$="/>
            <word name="$-"/>
            <word name="$~"/>
            <word name="$?"/>
<!--word name="$:"/-->
            <word name="$!"/>
            <word name="$@"/>
            <word name="$&lt;"/>
            <word name="$&gt;"/>
            <word name="$("/>
            <word name="$)"/>
            <word name="$_"/>
            <word name="$^L"/>
<!--word name="$$"/-->
            <word name="$^A"/>
            <word name="$^E"/>
            <word name="$^F"/>
            <word name="$^C"/>
            <word name="$^D"/>
            <word name="$^V"/>
            <word name="$^H"/>
            <word name="$^M"/>
            <word name="$^O"/>
            <word name="$^R"/>
            <word name="$^S"/>
            <word name="$^X"/>
            <word name="$^I"/>
            <word name="$^P"/>
            <word name="$^T"/>
            <word name="$^W"/>
            <word name="STDIN"/>
            <word name="STDOUT"/>
            <word name="STDERR"/>
            <word name="DATA"/>
            <word name="__FILE__"/>
            <word name="__PACKAGE__"/>
            <word name="__LINE__"/>
            <word name="__DIE__"/>
            <word name="__WARN__"/>
            <word name="ARGVOUT"/>
            <word name="stdin"/>
            <word name="stdout"/>
            <word name="stderr"/>
         </keywords>
         <regexp match="/\$\:\M[^\:]/x" region0="plVar"/>
      </scheme>

      <scheme name="perlVarInt">
         <regexp match="/ (::|')? \w+ /x" region1="plSymb"/>
      </scheme>

      <scheme name="perlVars">
         <regexp match="/\* [^\w\s\{\$\@\%\(]/x" region0="plVar"/>
         <inherit scheme="perlBuildinVars"/>
<!-- dynamic scheme's end regexp - cool :)  -->
         <block start="/[\@\%\*] \M [:'a-zA-Z]/x" end="//" scheme="perlVarInt" region="plVar"/>
         <block start="/[\$\%\@\*]+ \#?(\{)/x" end="/(\})/" scheme="perlVarIntEx" region="plVar" region00="PairStart" region10="PairEnd" region01="SymbolStrong" region11="SymbolStrong"/>
         <block start="/\$+ \#? \M [:'\w\$]/x" end="//" scheme="perlVarInt" region="plVar"/>
      </scheme>

      <scheme name="perlVarsEx">
         <block start="/[\@\%\*] \M [:'\w]/x" end="//" scheme="perlVarInt" region="plVar2" priority="low"/>
         <block start="/\$+ \#?\{/x" end="/\}/" scheme="perlVarInt" region="plVar2" region00="PairStart" region10="PairEnd" priority="low"/>
         <block start="/\$+ \#? \M ['\w]/x" end="//" scheme="perlVarInt" region="plVar" priority="low"/>
      </scheme>

      <scheme name="perlPod">
         <inherit scheme="NetAddresses"/>
         <regexp match="/^=(pod|cut)/" region0="plPodSym"/>
         <regexp match="/^=head[12]\s(.*)$/" region0="plPodSym" region1="plPodText"/>
         <regexp match="/^=(item|back|end)/" region0="plPodSym"/>
         <regexp match="/^=over\s+(\d+)/" region0="plPodSym" region1="plNumb"/>
         <regexp match="/[IBSCLFXZE]&lt;.*?&gt;/" region0="plPodSel"/>
      </scheme>

      <scheme name="perlBinNumb">
         <regexp match="/\b 0[bB][01]+ /ix" region0="NumberBin"/>
      </scheme>

      <scheme name="perlMETA"/>

      <scheme name="perlString">
         <inherit scheme="perlMETA"/>
<!-- Non-interpolated string ignore all escape-sequences except \' and \\ -->
         <regexp match="/\\['\\\/]/" region0="plSlash"/>
      </scheme>

      <scheme name="perlInterpolateString">
         <inherit scheme="perlMETA"/>
         <inherit scheme="NetAddresses"/>
         <regexp match="/\b0[xX][\da-fA-F]+\b/" region0="plNumb"/>
         <inherit scheme="FloatNumber"/>
         <inherit scheme="DecNumber"/>
         <inherit scheme="perlBinNumb"/>
<!-- cail: problems with s@ foo @ bar @ix;
              @ix is not a list :(( -->
         <inherit scheme="perlVarsEx"/>
<!-- <inherit scheme="perlBuildinVars"/> -->
         <regexp match="/\\[xX]0*[\da-fA-F]{1,2}/" region0="plSlash"/>
         <regexp match="/\\[0-7]{1,3}/" region0="plSlash"/>
         <regexp match="/\\[^xX0-7]/" region0="plSlash"/>
      </scheme>

      <scheme name="perlStrBr1">
         <regexp match="/\\[\(\)]/" region0="plSlash"/>
         <block scheme="perlStrBr1" start="/\(/" end="/\)/" region00="PairStart" region10="PairEnd"/>
      </scheme>

      <scheme name="perlStrBr2">
         <regexp match="/\\[\{\}]/" region0="plSlash"/>
         <block scheme="perlStrBr2" start="/\{/" end="/\}/" region00="PairStart" region10="PairEnd"/>
      </scheme>

      <scheme name="perlStrBr3">
         <regexp match="/(\\[\[\]])/" region0="plSlash"/>
         <block scheme="perlStrBr3" start="/\[/" end="/\]/" region00="PairStart" region10="PairEnd"/>
      </scheme>

      <scheme name="perlStrBr4">
         <regexp match="/(\\[&lt;\&gt;])/" region0="plSlash"/>
         <block scheme="perlStrBr3" start="/&lt;/" end="/\&gt;/" region00="PairStart" region10="PairEnd"/>
      </scheme>

      <scheme name="perlSubstComment">
<!-- are we need here  lowpriority="lowpriority" ??? -->
         <regexp match="/(^\s*|\s{2,})(\#\s.*$)/" region0="plComment"/>
      </scheme>

      <scheme name="perlSubstElements">
         <inherit scheme="perlInterpolateString"/>
         <inherit scheme="perlSubstComment"/>
      </scheme>

      <scheme name="perlIntStrBr1">
         <inherit scheme="perlSubstElements"/>
         <block scheme="perlIntStrBr1" start="/\(/" end="/\)/" region00="PairStart" region10="PairEnd"/>
      </scheme>

      <scheme name="perlIntStrBr2">
         <inherit scheme="perlSubstElements"/>
         <block scheme="perlIntStrBr2" start="/\{/" end="/\}/" region00="PairStart" region10="PairEnd"/>
      </scheme>

      <scheme name="perlIntStrBr3">
         <inherit scheme="perlSubstElements"/>
         <block scheme="perlIntStrBr3" start="/\[/" end="/\]/" region00="PairStart" region10="PairEnd"/>
      </scheme>

      <scheme name="perlIntStrBr4">
         <inherit scheme="perlSubstElements"/>
         <block scheme="perlIntStrBr3" start="/&lt;/" end="/\&gt;/" region00="PairStart" region10="PairEnd"/>
      </scheme>

      <scheme name="perlSubstMiddle1">
         <block start="/~ (s|tr|y) \s* \( /x" end="/\)/x" scheme="perlIntStrBr1" region10="plStringEdge"/>
         <inherit scheme="perlSubstComment"/>
         <regexp match="/\S+?/" region0="Error" priority="low"/>
      </scheme>

      <scheme name="perlSubstBlock1">
         <block start="/~ \M (s|tr|y) \s* \( /x" end="/\(/x" scheme="perlSubstMiddle1" region10="plStringEdge"/>
         <inherit scheme="perlIntStrBr1"/>
      </scheme>

      <scheme name="perlSubstMiddle2">
         <block start="/~ (s|tr|y) \s* \{ /x" end="/\}/x" scheme="perlIntStrBr2" region10="plStringEdge"/>
         <inherit scheme="perlSubstComment"/>
         <regexp match="/\S+/" region0="Error" priority="low"/>
      </scheme>

      <scheme name="perlSubstBlock2">
         <block start="/~ \M (s|tr|y) \s* \{ /x" end="/\{/x" scheme="perlSubstMiddle2" region10="plStringEdge"/>
         <inherit scheme="perlIntStrBr2"/>
      </scheme>

      <scheme name="perlSubstMiddle3">
         <block start="/~ (s|tr|y) \s* \[ /x" end="/\]/x" scheme="perlIntStrBr3" region10="plStringEdge"/>
         <inherit scheme="perlSubstComment"/>
         <regexp match="/\S+/" region0="Error" priority="low"/>
      </scheme>

      <scheme name="perlSubstBlock3">
         <block start="/~ \M (s|tr|y) \s* \[ /x" end="/\[/x" scheme="perlSubstMiddle3" region10="plStringEdge"/>
         <inherit scheme="perlIntStrBr3"/>
      </scheme>

      <scheme name="perlSubstMiddle4">
         <block start="/~ (s|tr|y) \s* &lt; /x" end="/\&gt;/x" scheme="perlIntStrBr4" region10="plStringEdge"/>
         <inherit scheme="perlSubstComment"/>
         <regexp match="/\S+/" region0="Error" priority="low"/>
      </scheme>

      <scheme name="perlSubstBlock4">
         <block start="/~ \M (s|tr|y) \s* &lt; /x" end="/&lt;/x" scheme="perlSubstMiddle4" region10="plStringEdge"/>
         <inherit scheme="perlIntStrBr4"/>
      </scheme>

      <scheme name="perlSubstBlock">
         <block start="/~ (s|tr|y) ([^\w\s]) /x" end="/\y2/x" scheme="perlSubstElements" region10="plStringEdge"/>
         <block start="/~ (s|tr|y) \s+ (\w) /x" end="/\y2/x" scheme="perlSubstElements" region10="plStringEdge"/>
         <inherit scheme="perlSubstElements"/>
      </scheme>

