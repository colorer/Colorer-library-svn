         <block start="/\b\M(((s|tr|y) \s* \{))/x" end="/(\} [mgeioxs]*)/x" scheme="perlSubstBlock2" region="plString" region01="PairStart" region10="PairEnd" region02="plStringEdge" region11="plStringEdge"/>
         <block start="/\b\M(((s|tr|y) \s* \[))/x" end="/(\] [mgeioxs]*)/x" scheme="perlSubstBlock3" region="plString" region01="PairStart" region10="PairEnd" region02="plStringEdge" region11="plStringEdge"/>
         <block start="/\b\M(((s|tr|y) \s* &lt;))/x" end="/(\&gt; [mgeioxs]*)/x" scheme="perlSubstBlock4" region="plString" region01="PairStart" region10="PairEnd" region02="plStringEdge" region11="plStringEdge"/>
         <block start="/\b\M(((s|tr|y) \s* ([^\w\s])))/x" end="/(\y4 [mgeioxs]*)/x" scheme="perlSubstBlock" region="plString" region01="PairStart" region10="PairEnd" region02="plStringEdge" region11="plStringEdge"/>
         <block start="/\b\M(((s|tr|y) \s+ (\w)))/x" end="/(\y4 [mgeioxs]*)/x" scheme="perlSubstBlock" region="plString" region01="PairStart" region10="PairEnd" region02="plStringEdge" region11="plStringEdge"/>
<!-- Timur: see `perldoc perlop`
       "Quote and Quote-like Operators " -->
         <block start="/\b(q[wqxr]\s*\()/" end="/(\))/" region="plString" scheme="perlIntStrBr1" region00="plStringEdge" region10="plStringEdge" region01="PairStart" region11="PairEnd"/>
         <block start="/\b(q[wqxr]\s*\{)/" end="/(\})/" region="plString" scheme="perlIntStrBr2" region00="plStringEdge" region10="plStringEdge" region01="PairStart" region11="PairEnd"/>
         <block start="/\b(q[wqxr]\s*\[)/" end="/(\])/" region="plString" scheme="perlIntStrBr3" region00="plStringEdge" region10="plStringEdge" region01="PairStart" region11="PairEnd"/>
         <block start="/\b(q[wqxr]\s*&lt;)/" end="/(\&gt;)/" region="plString" scheme="perlIntStrBr4" region00="plStringEdge" region10="plStringEdge" region01="PairStart" region11="PairEnd"/>
         <block start="/\b(q[wqxr]\s*([^\s]))/" end="/(\y2)/" region="plString" scheme="perlInterpolateString" region00="plStringEdge" region10="plStringEdge" region01="PairStart" region11="PairEnd"/>
         <block start="/\b(q(w)?\s*\()/" end="/(\))/" region="plString" scheme="perlStrBr1" region00="plStringEdge" region10="plStringEdge" region01="PairStart" region11="PairEnd"/>
         <block start="/\b(q(w)?\s*\{)/" end="/(\})/" region="plString" scheme="perlStrBr2" region00="plStringEdge" region10="plStringEdge" region01="PairStart" region11="PairEnd"/>
         <block start="/\b(q(w)?\s*\[)/" end="/(\])/" region="plString" scheme="perlStrBr3" region00="plStringEdge" region10="plStringEdge" region01="PairStart" region11="PairEnd"/>
         <block start="/\b(q(w)?\s*&lt;)/" end="/(\&gt;)/" region="plString" scheme="perlStrBr4" region00="plStringEdge" region10="plStringEdge" region01="PairStart" region11="PairEnd"/>
         <block start="/\b(q(w)?\s*([^\w\s]))/" end="/(\y3)/" region="plString" scheme="perlString" region00="plStringEdge" region10="plStringEdge" region01="PairStart" region11="PairEnd"/>
         <block start="/\b(m \s* \()/x" end="/(\)[cmiogxs]*)/" scheme="perlIntStrBr1" region="plString" region00="plStringEdge" region10="plStringEdge" region01="PairStart" region11="PairEnd"/>
         <block start="/\b(m \s* \{)/x" end="/(\}[cmiogxs]*)/" scheme="perlIntStrBr2" region="plString" region00="plStringEdge" region10="plStringEdge" region01="PairStart" region11="PairEnd"/>
         <block start="/\b(m \s* \[)/x" end="/(\][cmiogxs]*)/" scheme="perlIntStrBr3" region="plString" region00="plStringEdge" region10="plStringEdge" region01="PairStart" region11="PairEnd"/>
         <block start="/\b(m \s* &lt;)/x" end="/(\&gt;[cmiogxs]*)/" scheme="perlIntStrBr4" region="plString" region00="plStringEdge" region10="plStringEdge" region01="PairStart" region11="PairEnd"/>
         <block start="/\b(m \s* ([^\s\w]))/x" end="/(\y2[cmiogxs]*)/" scheme="perlInterpolateString" region="plString" region00="plStringEdge" region10="plStringEdge" region01="PairStart" region11="PairEnd"/>
<!-- Timur: hack against Ada maniacs -->
         <regexp match="/\c(&amp;|\w+)'[\w_']+\b/" region0="plVar"/>
<!-- operators -->
         <keywords region="plSymb">
            <symb name="!"/>
            <symb name="~"/>
            <symb name="\"/>
            <symb name="+"/>
            <symb name="-"/>
            <symb name="."/>
            <symb name="*"/>
            <symb name="/"/>
            <symb name=".."/>
            <symb name="%"/>
            <word name="lt"/>
            <word name="gt"/>
            <word name="le"/>
            <word name="ge"/>
            <word name="eq"/>
            <word name="ne"/>
            <word name="cmp"/>
            <symb name="&amp;"/>
            <symb name="|"/>
            <symb name="^"/>
            <symb name="?"/>
            <symb name=":"/>
            <symb name=","/>
            <symb name="="/>
            <word name="and"/>
            <word name="not"/>
            <word name="or"/>
            <word name="xor"/>
            <symb name="-&gt;"/>
            <symb name="&gt;"/>
            <symb name="&lt;"/>
            <symb name=";" region="plMainSymb"/>
            <symb name="{" region="Error"/>
            <symb name="}" region="Error"/>
            <symb name="(" region="Error"/>
            <symb name=")" region="Error"/>
            <symb name="[" region="Error"/>
            <symb name="]" region="Error"/>
         </keywords>
      </scheme>

      <scheme name="perlVarIntEx">
         <regexp match="/ (::|')? \w+ /x" region1="plSymb"/>
         <inherit scheme="perl"/>
      </scheme>

   </type>
