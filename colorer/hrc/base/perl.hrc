      <scheme name="perl">
<!-- linear comments -->
         <block start="/;?\#/" end="/$/" region="plComment" scheme="Comment"/>
<!-- Variables -->
         <inherit scheme="perlVars"/>
         <regexp match="/\b(sub) \s+ \M (?{SubOutline}[\w_']+) /x" region="plWord"/>
         <regexp match="/\b(package) \s+ \M (?{PackageOutline}[\w_']+) /x" region="plWord"/>
<!-- module::var -->
         <regexp match="/\b\w[\w\d]*\s*::\s*\w[\w\d]*\b/" region="plWord"/>
<!-- perl 5.6.0 number-encoded strings -->
         <regexp match="/\bv(\d+)(\.\d+)*\b/" region0="plString"/>
         <regexp match="/\b(\d+)(\.\d+){2,}\b/" region0="plString"/>
<!-- perl numbers  -->
         <inherit scheme="perlBinNumb"/>
         <regexp match="/\b0[xX][\da-fA-F_]+/" region0="plNumb"/>
         <regexp match="/\b\d[\d_]*\./" region0="plNumb"/>
         <regexp match="/\c[\d_]*\.?\d[\d_]*([eE][\-+]?[\d_]+)?/" region0="plNumb"/>
         <regexp match="/\b\d[\d_]+/" region0="plNumb"/>
<!-- strings -->
         <block start="/`/" end="/`/" scheme="perlInterpolateString" region="plString" region00="PairStart" region10="PairEnd"/>
         <block start="/&#34;/" end="/&#34;/" scheme="perlInterpolateString" region="plString" region00="PairStart" region10="PairEnd"/>
         <block start="/'/" end="/'/" scheme="perlString" region="plString" region00="PairStart" region10="PairEnd"/>
<!-- cail: paired block for matching -->
         <inherit scheme="PairedBrackets">
            <virtual scheme="PairedBrackets" subst-scheme="perl"/>
         </inherit>
<!-- cail: this block cracks the constructions
     $var = $var / $var / $var;
     $var =~ / $var /;
     if (/ $var /){};
     and so on...

     can't use ?regexp? - it conflicts with  $a = $b ? 1 : 2;
     really ?this? is deprecated construction in perl5, so...
   -->
         <block start="/( [\=\!]\~ |
                         [^&lt;\]\}\)\+\-\w\s\/] |
                         \bif | \bunless | \bsplit | ~ | ^)
                         \s* ((\/)) /x"
                         end="/(\y2[cmiogxes]*)/x" scheme="perlInterpolateString"
                         region="plString" region01="plSymb" region02="plStringEdge"
                         region03="PairStart" region10="plStringEdge" region11="PairEnd"/>

<!-- formatted output -->
         <block start="/^\s* \m (format) \b/x" end="/^\./" scheme="perlInterpolateString" region="plString" region01="plWord" region00="PairStart" region10="PairEnd"/>
<!-- <<out -->
         <block start="/&lt;&lt;\x22([a-zA-Z_][\w_]*)\x22/" end="/^(\y1)$/" scheme="perlInterpolateString" region="plString" region00="PairStart" region10="PairEnd" region01="plWord" region11="plWord"/>
         <block start="/&lt;&lt;'([a-zA-Z_][\w_]*)'/" end="/^(\y1)$/" scheme="Comment" region="plString" region00="PairStart" region10="PairEnd" region01="plWord" region11="plWord"/>
         <block start="/&lt;&lt;`([a-zA-Z_][\w_]*)`/" end="/^(\y1)$/" scheme="Comment" region="plString" region00="PairStart" region10="PairEnd" region01="plWord" region11="plWord"/>
         <block start="/&lt;&lt;([a-zA-Z_][\w_]*)/" end="/^(\y1)$/" scheme="perlInterpolateString" region="plString" region00="PairStart" region10="PairEnd" region01="plWord" region11="plWord"/>
         <block start="/&lt;&lt;\M([^\s\w_])/" end="/^$/" scheme="perlInterpolateString" region="plString" region00="PairStart" region10="PairEnd" region01="plWord" region11="plWord"/>
<!-- cail: perl in perl :)
