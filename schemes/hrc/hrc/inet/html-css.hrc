	</keywords>
</scheme>


<scheme name="khtmlPropertyNames" if="khtml-css"> 
	<keywords region="khtml" ignorecase="yes" worddiv='%css:worddiv-class;'>
		<word name="-khtml-border-bottom-left-radius"/>
