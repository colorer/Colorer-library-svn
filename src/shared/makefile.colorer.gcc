# gcc makefile

CPP     = @gcc
LINK    = @g++
RM      = @rm -f
obj     = o
DLLTOOL = @dlltool

obj_output_slash = $(obj_output)/

FLAGS = -s $(CYGWIN)
CPPFLAGS = -g -O2 -Wp,-I$(shared_path),-I$(unzipdir),-w -o $*.$(obj)  -c $(FLAGS) $(CPPFLAGS_CUSTOM) -D__unix__

include $(shared_path)makefile.zlib
include $(shared_path)_dependencies.dep
include $(shared_path)makefile.objs
